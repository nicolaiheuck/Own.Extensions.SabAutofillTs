{
	"name": "SAB AutoFill",
	"description": "Auto fill predefined profiles on SAB",
	"version": "1.0",
	"manifest_version": 3,
	"action": {
		"default_popup": "/views/popup.html"
	},
	"permissions": ["storage", "activeTab"],
	"content_scripts": [
		{
			"matches": [
				"*://booking.hessel.dk/*",
				"*://booking.dev-hessel.dk/*",
				"*://booking.staging-hessel.dk/*",
				"*://localhost/*"
			],
			"js": ["/js/contentScript.js"]
		}
	]
}
